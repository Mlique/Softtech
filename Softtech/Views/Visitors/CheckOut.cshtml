@model Softtech.ViewModel.VisitorsViewModel

@{
    ViewData["Title"] = "AddOrEdit";
    Layout = "~/Views/Shared/_VistorLayout.cshtml";
}

<div class="col-md-12 py-4">
    <form asp-action="CheckOut" method="post" enctype="multipart/form-data">
        <div asp-validation-summary="All" class="text-danger"></div>


        <div class="form-group col-md-6">
            <label>Time Out <span class="text-danger">*</span></label>
            <input asp-for="TimeOut" class="form-control floating" />
        </div>


        <div class="form-group submit-section mt-4">
            <button type="submit" class="btn btn-primary submit-btn">
                Check Out
            </button>
            <a asp-action="Index" asp-controller="Visitors" class="btn btn-secondary submit-btn">
                <i class="fa fa-reload btn-icon-prepend"></i>
                Back
            </a>
        </div>
    </form>
</div>
